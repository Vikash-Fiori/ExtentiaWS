{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.agel.mmts.vendormanagemdcc",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "dataSources": {
            "mainService": {
                "uri": "/AGEL_MMTS_API/api/v2/odata.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "com-agel-mmts-vendormanagemdcc-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "vendor-managemdcc",
                    "action": "manage",
                    "title": "Manage MDCC",
                    "subTitle": "",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "com.agel.mmts.vendormanagemdcc.view.App",
            "type": "XML",
            "async": true,
            "id": "fcl"
        },
        "config": {	
            "fullWidth": false	
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.agel.mmts.vendormanagemdcc.i18n.i18n"
                }
            },
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",	
                "settings": {	
                    "defaultOperationMode": "Server",	
                    "defaultBindingMode": "TwoWay",	
                    "defaultCountMode": "Inline",	
                    "refreshAfterChange": true,	
                    "useBatch": false	
                },
                "dataSource": "mainService",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.agel.mmts.vendormanagemdcc.view",
                "controlAggregation": "pages",
                "controlId": "fcl",
                "clearControlAggregation": false,
                "transition": "slide",	
                "bypassed": {	
                    "target": []	
                }
            },
            "routes": [
                {
                    "name": "RouteMaster",
                    "pattern": "",
                    "target": [
                        "TargetMaster"
                    ]
                },
                {
                    "pattern": "detail/{mdccID}/{parentItem}/{layout}",
                    "name": "detail",
                    "titleTarget": "",
                    "greedy": false,
                    "target": [
                        "TargetMaster",
                        "detail"
                    ]
                }
            ],
            "targets": {
                "TargetMaster": {
                    "viewType": "XML",
                    "transition": "slide",
                    "controlAggregation": "beginColumnPages",
                    "viewId": "Master",
                    "viewName": "Master"
                },
                "detail": {
                    "viewName": "Detail",
                    "viewLevel": 2,
                    "viewType": "XML",
                    "transition": "slide",
                    "controlAggregation": "midColumnPages"
                }
            }
        }
    }
}