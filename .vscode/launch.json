            "envFile": "${workspaceFolder}/tcengineer/.env1",
            "preLaunchTask": "Build agel-mmts-web"
        },
        {
            "name": "Start pmcg",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "cwd": "${workspaceFolder}/pmcg",
            "windows": {
                "runtimeExecutable": "npm.cmd"
            },
            "runtimeArgs": [
                "run-script",
                "start"
