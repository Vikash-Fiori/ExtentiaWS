<mvc:View xmlns:f2="sap.f"
    xmlns:vbm="sap.ui.vbm"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:uxap="sap.uxap"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:unified="sap.ui.unified"
    xmlns:tt="sap.ui.table"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns="sap.m" controllerName="com.agel.mmts.raiseconsumptionporequest.controller.RaiseConsumptionCancelPage" height="100%">
    <uxap:ObjectPageLayout id="ObjectPageLayout" showTitleInHeaderContent="true" upperCaseAnchorBar="false" headerContentPinnable="true" toggleHeaderOnTitleClick="true" busy="{objectViewModel>/busy}">
        <uxap:headerTitle>
            <uxap:ObjectPageDynamicHeaderTitle>
                <uxap:breadcrumbs>
                    <Breadcrumbs id="breadcrumbsId" currentLocationText="{i18n>ConsumptionPostingItems}">
                        <Link text="{i18n>ConsumptionReservationItems}" press=".handleToAllPOBreadcrumPress"/>
                    </Breadcrumbs>
                </uxap:breadcrumbs>
                <uxap:expandedHeading>
                    <FlexBox direction="Column" renderType="Bare">
                        <Title text="{i18n>ConsumptionPostingItems}" wrapping="true" titleStyle="H4" class="sapUiTinyMarginTopBottom"/>
                        <Title text="{SONumber}" wrapping="true" titleStyle="H3" />
                    </FlexBox>
                </uxap:expandedHeading>
                <uxap:snappedHeading>
                    <FlexBox direction="Column" renderType="Bare">
                        <Title text="{i18n>ConsumptionPostingItems}" wrapping="true" titleStyle="H4" class="sapUiTinyMarginTopBottom"/>
                        <Title text="{SONumber}" wrapping="true" titleStyle="H3"/>
                    </FlexBox>
                </uxap:snappedHeading>
                <uxap:expandedContent>
                </uxap:expandedContent>
                <uxap:snappedTitleOnMobile>
                    <Title text="{i18n>SONumber}- {SONumber}" wrapping="true" titleStyle="Auto"/>
                </uxap:snappedTitleOnMobile>
                <uxap:actions>
                    <HBox visible="true">
                        <!-- <Button text="Done" press="onPressDone" type="Accept" visible="{objectViewModel>/doneButton}"/> -->
                        <Title text="" />
                        <Button text="Cancel" press="onPressCancelConsumptionPosting" type="Emphasized"/>
                    </HBox>
                </uxap:actions>
            </uxap:ObjectPageDynamicHeaderTitle>
        </uxap:headerTitle>
        <uxap:headerContent>
            <VBox binding="{ConsumptionPostingReserve}">

            </VBox>
        </uxap:headerContent>
        <uxap:sections>
            <!-- Line Items Section -->
            <uxap:ObjectPageSection titleUppercase="false">
                <uxap:subSections>
                    <uxap:ObjectPageSubSection >
                        <smartTable:SmartTable class="sapUiResponsiveContentPadding" id="idIssueTable" editable="false" placeToolbarInTable="true" enableAutoBinding="true" entitySet="ConsumptionPostingSet" tableBindingPath="ConsumptionPostingReserve/ConsumedMaterialParent" showFullScreenButton="true" requestAtLeastFields="ID" header="Consumption Posting" showRowCount="true" tableType="ResponsiveTable" demandPopin="false" useExportToExcel="false">
                            <Table sticky="ColumnHeaders,HeaderToolbar" alternateRowColors="true">
                                <columns>
                                    <Column >
                                        <customData>
                                            <core:CustomData key="p13nData" value='\{"columnKey": "MaterialDocumentNumber", "leadingProperty": "MaterialDocumentNumber",
"columnIndex": 0 ,"sortProperty": "MaterialDocumentNumber","filterProperty": "MaterialDocumentNumber"}'/>
                                        </customData>
                                        <Text text="{i18n>MaterialDocumentNumber}"/>
                                    </Column>
                                    <Column >
                                        <customData>
                                            <core:CustomData key="p13nData" value='\{"columnKey": "MovementType", "leadingProperty": "MovementType",
"columnIndex": 1 ,"sortProperty": "MovementType","filterProperty": "MovementType"}'/>
                                        </customData>
                                        <Text text="{i18n>MaterialDocumentNumber}"/>
                                    </Column>
                                    <Column >
                                        <customData>
                                            <core:CustomData key="p13nData" value='\{"columnKey": "MaterialCode", "leadingProperty": "MaterialCode",
"columnIndex": 2 ,"sortProperty": "MaterialCode","filterProperty": "MaterialCode"}'/>
                                        </customData>
                                        <Text text="{i18n>MaterialCode}"/>
                                    </Column>
                                    <Column >
                                        <customData>
                                            <core:CustomData key="p13nData" value='\{"columnKey": "Plant", "leadingProperty": "Plant",
"columnIndex": 3 ,"sortProperty": "Plant","filterProperty": "Plant"}'/>
                                        </customData>
                                        <Text text="{i18n>Plant}"/>
                                    </Column>
                                    <Column >
                                        <customData>
                                            <core:CustomData key="p13nData" value='\{"columnKey": "StorageLocation", "leadingProperty": "StorageLocation",
"columnIndex": 4 ,"sortProperty": "StorageLocation","filterProperty": "StorageLocation"}'/>
                                        </customData>
                                        <Text text="{i18n>Plant}"/>
                                    </Column>
                                    <Column >
                                        <customData>
                                            <core:CustomData key="p13nData" value='\{"columnKey": "Quantity", "leadingProperty": "Quantity",
"columnIndex": 5 ,"sortProperty": "Quantity","filterProperty": "Quantity"}'/>
                                        </customData>
                                        <Text text="{i18n>Quantity}"/>
                                    </Column>
                                    <Column >
                                        <customData>
                                            <core:CustomData key="p13nData" value='\{"columnKey": "ReservationNumber", "leadingProperty": "ReservationNumber",
"columnIndex": 6 ,"sortProperty": "ReservationNumber","filterProperty": "ReservationNumber"}'/>
                                        </customData>
                                        <Text text="{i18n>ReservationNumber}"/>
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem >
                                        <cells>
                                            <ObjectIdentifier title="{MaterialDocumentNumber}"/>
                                            <Text text="{MovementType}"/>
                                            <Text text="{MaterialCode}"/>
                                            <Text text="{Plant}"/>
                                            <Text text="{StorageLocation}"/>
                                            <Text text="{Quantity}"/>
                                            <Text text="{ReservationNumber}"/>
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                        </smartTable:SmartTable>
                    </uxap:ObjectPageSubSection>
                </uxap:subSections>
            </uxap:ObjectPageSection>
        </uxap:sections>
    </uxap:ObjectPageLayout>
</mvc:View> 