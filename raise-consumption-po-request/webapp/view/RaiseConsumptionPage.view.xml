<mvc:View controllerName="com.agel.mmts.raiseconsumptionporequest.controller.RaiseConsumptionPage" height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f">
    <f:DynamicPage headerExpanded="true" busy="{objectViewModel>/busy}">
        <!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title id="pageTitle" text="Raise Consumption Posting Request"/>
                </f:heading>
                <f:actions>
                   <!-- <Button text="Raise Return Material Request" press="onConfirmRequest" type="Emphasized"/> -->
                </f:actions>
            </f:DynamicPageTitle>
        </f:title>
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="false">
                <f:content>
                    <HBox alignContent="SpaceAround">
                        <Input liveChange="onChangeSONumber"    placeholder="Enter SO Number" type="Number"  id="idSoNumber" width="90%"  ></Input>
                        <Button id="SObtnSubmit" text="Submit" press="onSearch" type="Emphasized"/>
                    </HBox>
                </f:content>              
            </f:DynamicPageHeader>
        </f:header>
        <!-- DynamicPage Content -->
        <f:content>
            <!-- <core:Fragment fragmentName="com.agel.mmts.returnmaterial.view.fragments.RequestTable" type="XML"/> -->
        </f:content>
    </f:DynamicPage>
</mvc:View>

