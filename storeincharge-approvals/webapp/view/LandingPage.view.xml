                <items>
                    <IconTabFilter id="idListTitle" text="{i18n>approvals}" key="ListKey" count="{oIconTabCountModel>/confirmCount}"> -->
                        <core:Fragment fragmentName="com.agel.mmts.storeinchargeapprovals.view.fragments.ListTable" type="XML"/>
                    <!-- </IconTabFilter>
                </items>
            </IconTabBar> -->
        </f:content>
    </f:DynamicPage>
</mvc:View>

