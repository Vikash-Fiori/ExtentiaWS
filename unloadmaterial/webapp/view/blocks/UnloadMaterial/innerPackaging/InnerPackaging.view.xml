<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:tt="sap.ui.table" controllerName="com.agel.mmts.unloadmaterial.view.blocks.UnloadMaterial.innerPackaging.InnerPackagingController">
    
  <Table id="idInnerPackaging" items="{path: 'InnerPackagings'}" autoPopinMode="true" growingThreshold="5" growing="true" fixedLayout="false">
    
        <columns>
            <Column>
                <Text text="Inner Packaging"/>
            </Column>
            <Column>
                <Text text="Line Item"/>
            </Column>
            <Column>
                <Text text="Packaging Qty"/>
            </Column>
            <Column>
                <Text text="Received Qty"/>
            </Column>
            <Column>
                <Text text="UOM"/>
            </Column>
            <Column>
              <Text text="Status"/>
            </Column>
        </columns>
        <items>
            <ColumnListItem>
                <cells>
                    <ObjectIdentifier title="{PackagingType}"/>
                    <ObjectIdentifier title="{LineNumber}"/>
                    <Text text="{PackagingQty}"/>
                    <Text text=""/>
                    <Text text="{UOM}"/>
                    <!-- <core:Icon
                        src="sap-icon://sys-help-2" class="size1" binding="{QrCode}" visible="{= ${QRNumber}}" color="#031E48" press="onViewQRCodePress">
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon> -->
                    <core:Icon src="sap-icon://accept" class="size1" color="#087137" >
                        <core:layoutData>
                            <FlexItemData growFactor="1" />
                        </core:layoutData>
                    </core:Icon>
                </cells>
            </ColumnListItem>
        </items>
    </Table> 
</mvc:View>


